{
  "name": "elf",
  "displayName": "elf",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "elfReader",
          "title": "ELF Reader",
          "icon": "$(file-binary)"
        }
      ]
    },
    "views": {
      "elfReader": [
        {
          "id": "elfHeaders",
          "name": "ELF headers",
          "contextualTitle": "ELF Headers"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "elfHeaders",
        "contents": "No ELF file selected.\nSelect a binary file in the file explorer and hit the refresh button to get started! ðŸ”¥"
      }
    ],
    "commands": [
      {
        "command": "elfReader.refresh",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "elfReader.inspectHeader",
        "title": "Inspect header",
        "icon": "$(inspect)"
      },
      {
        "command": "elfReader.inspectProgram",
        "title": "Inspect program",
        "icon": "$(inspect)"
      },
      {
        "command": "elfReader.inspectSection",
        "title": "Inspect section",
        "icon": "$(inspect)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "elfReader.refresh",
          "when": "view == elfHeaders",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "elfy": "^1.0.0"
  }
}
